{
  "cSpell.words": [
    "SAVEPOINT",
    "Unpublish",
    "Upsert",
    "datadata",
    "dbaeumer",
    "deno",
    "dependabot",
    "esbenp",
    "gnuplot",
    "graphiql",
    "graphqlid",
    "graphviz",
    "jonasb",
    "nvmrc",
    "pgcli",
    "pnpm",
    "postgres",
    "queryable",
    "uuidv"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "files.associations": {
    "rush.json": "jsonc",
    "command-line.json": "jsonc",
    "common-versions.json": "jsonc",
    "experiments.json": "jsonc",
    "version-policies.json": "jsonc"
  },
  "files.insertFinalNewline": true,
  "json.schemas": [
    {
      "fileMatch": ["version-policies.json"],
      "url": "https://raw.githubusercontent.com/microsoft/rushstack/master/apps/rush-lib/src/schemas/version-policies.schema.json"
    }
  ]
}
