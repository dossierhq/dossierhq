{
  "name": "@dossierhq/design",
  "version": "0.7.10",
  "description": "The design system for Dossier.",
  "keywords": [
    "dossier",
    "cms",
    "design-system",
    "react"
  ],
  "homepage": "https://dossierhq.dev",
  "author": {
    "name": "Jonas Bengtsson"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./lib/index.js",
    "./main.css": "./lib/main.css"
  },
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "npm run prettier:check && ./scripts/generate-icons.cjs && eslint && tsc && (npm run css:build 2>&1)",
    "build:watch": "tsc --watch",
    "clean": "rm -rf lib/",
    "css:build": "sass src/styles/main.scss lib/main.css",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "start": "storybook dev -p 6007 --no-open",
    "storybook:build": "storybook build --quiet",
    "test": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dossierhq/dossierhq.git",
    "directory": "libraries/design"
  },
  "dependencies": {
    "@creativebulma/bulma-badge": "~1.0.1",
    "@fortawesome/free-solid-svg-icons": "~6.7.2",
    "@react-aria/overlays": "~3.25.0",
    "lodash": "~4.17.21",
    "react-aria-components": "~1.6.0",
    "tslib": "~2.8.1"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19"
  },
  "devDependencies": {
    "@dossierhq/eslint-config": "workspace:*",
    "@dossierhq/typescript-config": "workspace:*",
    "@ianvs/prettier-plugin-sort-imports": "~4.4.1",
    "@storybook/addon-actions": "~8.5.3",
    "@storybook/addon-docs": "~8.5.3",
    "@storybook/addon-essentials": "~8.5.3",
    "@storybook/addon-links": "~8.5.3",
    "@storybook/preset-scss": "~1.0.3",
    "@storybook/react": "~8.5.3",
    "@storybook/react-vite": "~8.5.3",
    "@storybook/test": "~8.5.3",
    "@types/lodash": "~4.17.15",
    "@types/react": "19.0.8",
    "@types/react-dom": "~19.0.3",
    "@vitejs/plugin-react": "~4.3.4",
    "bulma": "~0.9.4",
    "eslint": "~9.20.0",
    "mini-svg-data-uri": "~1.4.4",
    "prettier": "3.4.2",
    "react": "~19.0.0",
    "react-dom": "~19.0.0",
    "react-stately": "~3.35.0",
    "sass": "~1.83.4",
    "sass-loader": "~16.0.4",
    "storybook": "~8.5.3",
    "svgo": "~3.3.2",
    "typescript": "~5.7.3",
    "vite": "~6.1.0"
  }
}
