{
  "name": "@dossierhq/react-components2",
  "version": "0.0.1",
  "description": "React components for creating a headless CMS using Dossier.",
  "keywords": [
    "dossier",
    "cms",
    "react"
  ],
  "homepage": "https://dossierhq.dev",
  "author": {
    "name": "Jonas Bengtsson"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./lib/index.js",
    "./main.css": "./lib/main.css"
  },
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "npm run prettier:check && heft build --clean && postcss src/globals.css -o lib/main.css && npm run eslint:check && npm run test",
    "build:watch": "heft build-watch",
    "build-storybook": "storybook build --quiet",
    "clean": "heft clean",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "start": "storybook dev -p 6008 --no-open",
    "test": "vitest",
    "test:update-snapshot": "vitest --update",
    "test:watch": "vitest --watch",
    "test:storybook": "test-storybook --url http://127.0.0.1:6008 -s --browsers firefox chromium webkit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dossierhq/dossierhq.git",
    "directory": "libraries/react-components2"
  },
  "bugs": {
    "url": "https://github.com/dossierhq/dossierhq/issues"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "~1.1.1",
    "@radix-ui/react-collapsible": "~1.1.0",
    "@radix-ui/react-dialog": "~1.1.1",
    "@radix-ui/react-dropdown-menu": "~2.1.1",
    "@radix-ui/react-label": "~2.1.0",
    "@radix-ui/react-popover": "~1.1.1",
    "@radix-ui/react-slot": "~1.1.0",
    "class-variance-authority": "~0.7.0",
    "clsx": "~2.1.1",
    "cmdk": "~1.0.0",
    "lodash": "~4.17.21",
    "lucide-react": "~0.396.0",
    "swr": "~2.2.5",
    "tailwind-merge": "~2.3.0",
    "tailwindcss-animate": "~1.0.7",
    "tslib": "~2.6.3"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "~1.5.0",
    "@dossierhq/core": "workspace:*",
    "@dossierhq/heft-node-rig": "workspace:*",
    "@dossierhq/server": "workspace:*",
    "@dossierhq/sql.js": "workspace:*",
    "@ianvs/prettier-plugin-sort-imports": "~4.2.1",
    "@rushstack/heft": "0.66.18",
    "@storybook/addon-actions": "~8.1.10",
    "@storybook/addon-docs": "~8.1.10",
    "@storybook/addon-essentials": "~8.1.10",
    "@storybook/addon-interactions": "~8.1.10",
    "@storybook/addon-links": "~8.1.10",
    "@storybook/react": "~8.1.10",
    "@storybook/react-vite": "~8.1.10",
    "@storybook/test": "~8.1.10",
    "@storybook/test-runner": "~0.19.0",
    "@types/lodash": "~4.17.5",
    "@types/react": "18.3.3",
    "@types/react-dom": "~18.3.0",
    "@types/sql.js": "~1.4.9",
    "@typescript-eslint/eslint-plugin": "~7.13.1",
    "@typescript-eslint/parser": "~7.13.1",
    "@vitejs/plugin-react": "~4.3.1",
    "autoprefixer": "~10.4.19",
    "chromatic": "~11.5.4",
    "eslint": "~8.57.0",
    "eslint-plugin-react": "~7.34.3",
    "eslint-plugin-react-compiler": "0.0.0-experimental-51a85ea-20240601",
    "eslint-plugin-react-hooks": "~4.6.2",
    "eslint-plugin-storybook": "~0.8.0",
    "playground-example-generator": "workspace:*",
    "postcss": "~8.4.38",
    "postcss-cli": "~11.0.0",
    "prettier": "3.3.2",
    "prettier-plugin-tailwindcss": "~0.6.5",
    "react": "~18.3.1",
    "react-dom": "~18.3.1",
    "sql.js": "~1.10.3",
    "storybook": "~8.1.10",
    "tailwindcss": "~3.4.4",
    "typescript": "~5.5.2",
    "vite": "~5.3.1",
    "vitest": "~1.6.0"
  }
}
