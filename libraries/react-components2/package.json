{
  "name": "@dossierhq/react-components2",
  "version": "0.7.17",
  "description": "React components for creating a headless CMS using Dossier.",
  "keywords": [
    "dossier",
    "cms",
    "react"
  ],
  "homepage": "https://dossierhq.dev",
  "author": {
    "name": "Jonas Bengtsson"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./lib/index.js",
    "./main.css": "./lib/main.css"
  },
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && postcss src/globals.css -o lib/main.css",
    "build:watch": "tsc --watch",
    "build-storybook": "storybook build --quiet",
    "lint": "npm run prettier:check && npm run eslint:check",
    "clean": "rm -rf lib/",
    "chromatic": "chromatic",
    "eslint:check": "eslint --max-warnings=0",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "start": "storybook dev -p 6008 --no-open",
    "test": "vitest",
    "test:update-snapshot": "vitest --update",
    "test:watch": "vitest --watch",
    "test:storybook": "test-storybook --url http://127.0.0.1:6008 -s --browsers firefox chromium webkit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dossierhq/dossierhq.git",
    "directory": "libraries/react-components2"
  },
  "bugs": {
    "url": "https://github.com/dossierhq/dossierhq/issues"
  },
  "dependencies": {
    "@dossierhq/leaflet": "workspace:*",
    "@radix-ui/react-alert-dialog": "~1.1.6",
    "@radix-ui/react-collapsible": "~1.1.3",
    "@radix-ui/react-dialog": "~1.1.6",
    "@radix-ui/react-dropdown-menu": "~2.1.6",
    "@radix-ui/react-label": "~2.1.2",
    "@radix-ui/react-popover": "~1.1.6",
    "@radix-ui/react-slot": "~1.1.2",
    "@radix-ui/react-toggle": "~1.1.2",
    "@radix-ui/react-toggle-group": "~1.1.2",
    "@radix-ui/react-visually-hidden": "~1.1.2",
    "class-variance-authority": "~0.7.1",
    "clsx": "~2.1.1",
    "cmdk": "~1.0.4",
    "leaflet": "~1.9.4",
    "lodash": "~4.17.21",
    "lucide-react": "~0.487.0",
    "next-themes": "~0.4.6",
    "react-resizable-panels": "~2.1.7",
    "sonner": "2.0.1",
    "swr": "~2.3.3",
    "tailwind-merge": "~3.0.2",
    "tslib": "~2.8.1"
  },
  "peerDependencies": {
    "@dossierhq/core": "workspace:*",
    "react": ">=19",
    "react-dom": ">=19"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "~3.2.6",
    "@dossierhq/core": "workspace:*",
    "@dossierhq/eslint-config": "workspace:*",
    "@dossierhq/server": "workspace:*",
    "@dossierhq/sql.js": "workspace:*",
    "@dossierhq/test-data": "workspace:*",
    "@dossierhq/typescript-config": "workspace:*",
    "@storybook/addon-actions": "~8.6.11",
    "@storybook/addon-docs": "~8.6.11",
    "@storybook/addon-essentials": "~8.6.11",
    "@storybook/addon-interactions": "~8.6.11",
    "@storybook/addon-links": "~8.6.11",
    "@storybook/react": "~8.6.11",
    "@storybook/react-vite": "~8.6.11",
    "@storybook/test": "~8.6.11",
    "@storybook/test-runner": "~0.22.0",
    "@tailwindcss/vite": "^4.0.12",
    "@types/lodash": "~4.17.16",
    "@types/react": "19.0.12",
    "@types/react-dom": "~19.0.4",
    "@types/sql.js": "~1.4.9",
    "@vitejs/plugin-react": "~4.3.4",
    "chromatic": "~11.27.0",
    "concurrently": "~9.1.2",
    "eslint": "~9.23.0",
    "http-server": "~14.1.1",
    "postcss": "~8.5.3",
    "postcss-cli": "~11.0.0",
    "postcss-remove-rules": "^1.0.0",
    "prettier-plugin-tailwindcss": "~0.6.11",
    "react": "~19.0.0",
    "react-dom": "~19.0.0",
    "sql.js": "~1.12.0",
    "storybook": "~8.6.11",
    "tailwindcss": "~4.0.12",
    "tailwindcss-animate": "~1.0.7",
    "typescript": "~5.8.2",
    "vite": "~6.2.1",
    "vitest": "~3.0.9",
    "wait-on": "~8.0.2"
  }
}
