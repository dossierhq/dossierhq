{
  "name": "@dossierhq/cloudinary",
  "version": "0.7.21",
  "description": "A field editor Dossier for Cloudinary images.",
  "keywords": [
    "dossier",
    "cms",
    "cloudinary"
  ],
  "homepage": "https://dossierhq.dev",
  "author": {
    "name": "Jonas Bengtsson"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./lib/index.js"
  },
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "eslint --max-warnings=0",
    "lint:staged": "lint-staged",
    "clean": "rm -rf lib/",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dossierhq/dossierhq.git",
    "directory": "libraries/cloudinary"
  },
  "dependencies": {
    "@cloudinary/url-gen": "~1.22.0",
    "@dossierhq/design": "workspace:*",
    "tslib": "~2.8.1"
  },
  "peerDependencies": {
    "@dossierhq/core": "workspace:*",
    "react": ">=18"
  },
  "devDependencies": {
    "@dossierhq/core": "workspace:*",
    "@dossierhq/eslint-config": "workspace:*",
    "@dossierhq/react-components": "workspace:*",
    "@dossierhq/typescript-config": "workspace:*",
    "@types/react": "19.2.14",
    "eslint": "~9.39.2",
    "react": "~19.2.4",
    "typescript": "~5.9.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,cjs,mjs}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ]
  }
}
