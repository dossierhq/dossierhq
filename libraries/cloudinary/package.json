{
  "name": "@dossierhq/cloudinary",
  "version": "0.7.21",
  "description": "A field editor Dossier for Cloudinary images.",
  "keywords": [
    "dossier",
    "cms",
    "cloudinary"
  ],
  "homepage": "https://dossierhq.dev",
  "author": {
    "name": "Jonas Bengtsson"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./lib/index.js"
  },
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "npm run prettier:check && npm run eslint:check",
    "clean": "rm -rf lib/",
    "eslint:check": "eslint --max-warnings=0",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dossierhq/dossierhq.git",
    "directory": "libraries/cloudinary"
  },
  "dependencies": {
    "@cloudinary/url-gen": "~1.21.0",
    "@dossierhq/design": "workspace:*",
    "tslib": "~2.8.1"
  },
  "peerDependencies": {
    "@dossierhq/core": "workspace:*",
    "react": ">=18"
  },
  "devDependencies": {
    "@dossierhq/core": "workspace:*",
    "@dossierhq/eslint-config": "workspace:*",
    "@dossierhq/react-components": "workspace:*",
    "@dossierhq/typescript-config": "workspace:*",
    "@types/react": "19.2.2",
    "eslint": "~9.38.0",
    "react": "~19.2.0",
    "typescript": "~5.8.3"
  }
}
