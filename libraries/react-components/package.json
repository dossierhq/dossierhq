{
  "name": "@dossierhq/react-components",
  "version": "0.2.12",
  "description": "React components for creating a headless CMS using Dossier.",
  "keywords": [
    "dossier",
    "cms",
    "schema",
    "headless"
  ],
  "homepage": "https://dossierhq.dev",
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./main.css": "./lib/main.css"
  },
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "npm run prettier:check && heft build --clean && npm run test",
    "build:watch": "heft build --watch",
    "clean": "heft clean",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "start": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build --quiet",
    "test": "vitest",
    "test:update-snapshot": "vitest --update",
    "test:watch": "vitest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dossierhq/dossierhq.git",
    "directory": "libraries/react-components"
  },
  "devDependencies": {
    "@dossierhq/sqlite3": "workspace:*",
    "@dossierhq/heft-node-rig": "workspace:*",
    "@dossierhq/server": "workspace:*",
    "@rushstack/heft": "0.50.0",
    "@storybook/addon-actions": "~7.0.2",
    "@storybook/addon-essentials": "~7.0.2",
    "@storybook/addon-links": "~7.0.2",
    "@storybook/react": "~7.0.2",
    "@storybook/react-vite": "~7.0.2",
    "@types/leaflet": "~1.9.3",
    "@types/lodash": "~4.14.192",
    "@types/react-dom": "~18.0.11",
    "@types/react": "~18.0.33",
    "@types/uuid": "~9.0.1",
    "@typescript-eslint/eslint-plugin": "~5.57.1",
    "@typescript-eslint/parser": "~5.57.1",
    "@vitejs/plugin-react": "~3.1.0",
    "eslint": "~8.38.0",
    "eslint-plugin-import": "~2.27.5",
    "eslint-plugin-react": "~7.32.2",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-plugin-storybook": "~0.6.11",
    "prettier": "2.8.7",
    "storybook": "~7.0.2",
    "typescript": "~5.0.4",
    "uuid": "~9.0.0",
    "vite": "~4.2.1",
    "vitest": "~0.29.8"
  },
  "dependencies": {
    "@dossierhq/core": "workspace:*",
    "@dossierhq/design": "workspace:*",
    "@dossierhq/leaflet": "workspace:*",
    "@lexical/code": "0.9.1",
    "@lexical/link": "0.9.1",
    "@lexical/list": "0.9.1",
    "@lexical/react": "0.9.1",
    "@lexical/rich-text": "0.9.1",
    "@lexical/selection": "0.9.1",
    "@lexical/utils": "0.9.1",
    "leaflet": "~1.9.3",
    "lexical": "0.9.1",
    "lodash": "~4.17.21",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "swr": "~2.1.2",
    "tslib": "~2.5.0"
  }
}
