{
  "name": "@dossierhq/react-components",
  "version": "0.4.4",
  "description": "React components for creating a headless CMS using Dossier.",
  "keywords": [
    "dossier",
    "cms",
    "schema",
    "headless"
  ],
  "homepage": "https://dossierhq.dev",
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./main.css": "./lib/main.css"
  },
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "npm run prettier:check && heft build --clean && npm run test",
    "build:watch": "heft build-watch",
    "clean": "heft clean",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "start": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build --quiet",
    "test": "vitest",
    "test:update-snapshot": "vitest --update",
    "test:watch": "vitest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dossierhq/dossierhq.git",
    "directory": "libraries/react-components"
  },
  "devDependencies": {
    "@dossierhq/better-sqlite3": "workspace:*",
    "@dossierhq/heft-node-rig": "workspace:*",
    "@dossierhq/server": "workspace:*",
    "@rushstack/heft": "0.59.0",
    "@storybook/addon-actions": "~7.4.2",
    "@storybook/addon-essentials": "~7.4.2",
    "@storybook/addon-links": "~7.4.2",
    "@storybook/react": "~7.4.2",
    "@storybook/react-vite": "~7.4.2",
    "@types/leaflet": "~1.9.4",
    "@types/lodash": "~4.14.198",
    "@types/react-dom": "~18.2.7",
    "@types/react": "~18.2.21",
    "@types/uuid": "~9.0.4",
    "@typescript-eslint/eslint-plugin": "~6.7.0",
    "@typescript-eslint/parser": "~6.7.0",
    "@vitejs/plugin-react": "~4.0.4",
    "eslint": "~8.49.0",
    "eslint-plugin-import": "~2.28.1",
    "eslint-plugin-react": "~7.33.2",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-plugin-storybook": "~0.6.13",
    "playground-example-generator": "workspace:*",
    "prettier": "3.0.3",
    "storybook": "~7.4.2",
    "typescript": "~5.2.2",
    "uuid": "~9.0.1",
    "vite": "~4.4.9",
    "vitest": "~0.34.4"
  },
  "dependencies": {
    "@dossierhq/core": "workspace:*",
    "@dossierhq/design": "workspace:*",
    "@dossierhq/leaflet": "workspace:*",
    "@lexical/code": "0.12.2",
    "@lexical/link": "0.12.2",
    "@lexical/list": "0.12.2",
    "@lexical/react": "0.12.2",
    "@lexical/rich-text": "0.12.2",
    "@lexical/selection": "0.12.2",
    "@lexical/utils": "0.12.2",
    "leaflet": "~1.9.4",
    "lexical": "0.12.2",
    "lodash": "~4.17.21",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "swr": "~2.2.2",
    "tslib": "~2.6.2"
  }
}
