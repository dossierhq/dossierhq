{
  "name": "@dossierhq/react-components",
  "version": "0.7.12",
  "description": "React components for creating a headless CMS using Dossier.",
  "keywords": [
    "dossier",
    "cms",
    "schema",
    "headless"
  ],
  "homepage": "https://dossierhq.dev",
  "author": {
    "name": "Jonas Bengtsson"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./lib/index.js",
    "./main.css": "./lib/main.css"
  },
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "npm run prettier:check && eslint --max-warnings=0 && tsc && npm run test && cp src/main.css lib/main.css",
    "build:watch": "tsc --watch",
    "clean": "rm -rf lib/",
    "eslint:check": "eslint --max-warnings=0",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "start": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build --quiet",
    "test": "vitest",
    "test:update-snapshot": "vitest --update",
    "test:watch": "vitest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dossierhq/dossierhq.git",
    "directory": "libraries/react-components"
  },
  "dependencies": {
    "@dossierhq/design": "workspace:*",
    "@dossierhq/leaflet": "workspace:*",
    "@lexical/code": "0.14.3",
    "@lexical/link": "0.14.3",
    "@lexical/list": "0.14.3",
    "@lexical/react": "0.14.3",
    "@lexical/rich-text": "0.14.3",
    "@lexical/selection": "0.14.3",
    "@lexical/utils": "0.14.3",
    "leaflet": "~1.9.4",
    "lexical": "0.14.3",
    "lodash": "~4.17.21",
    "swr": "~2.3.2",
    "tslib": "~2.8.1"
  },
  "peerDependencies": {
    "@dossierhq/core": "workspace:*",
    "react": ">=19",
    "react-dom": ">=19"
  },
  "devDependencies": {
    "@dossierhq/better-sqlite3": "workspace:*",
    "@dossierhq/core": "workspace:*",
    "@dossierhq/eslint-config": "workspace:*",
    "@dossierhq/server": "workspace:*",
    "@dossierhq/typescript-config": "workspace:*",
    "@ianvs/prettier-plugin-sort-imports": "~4.4.1",
    "@storybook/addon-actions": "~8.5.3",
    "@storybook/addon-essentials": "~8.5.3",
    "@storybook/addon-links": "~8.5.3",
    "@storybook/react": "~8.5.3",
    "@storybook/react-vite": "~8.5.3",
    "@storybook/test": "~8.5.3",
    "@types/leaflet": "~1.9.14",
    "@types/lodash": "~4.17.15",
    "@types/react": "19.0.8",
    "@types/react-dom": "~19.0.3",
    "@types/uuid": "~10.0.0",
    "@vitejs/plugin-react": "~4.3.4",
    "eslint": "~9.20.0",
    "playground-example-generator": "workspace:*",
    "prettier": "3.5.0",
    "react": "~19.0.0",
    "react-dom": "~19.0.0",
    "storybook": "~8.5.3",
    "typescript": "~5.7.3",
    "uuid": "~11.0.5",
    "vite": "~6.1.0",
    "vitest": "~3.0.5"
  }
}
