{
  "name": "@dossierhq/react-components",
  "version": "0.7.6",
  "description": "React components for creating a headless CMS using Dossier.",
  "keywords": [
    "dossier",
    "cms",
    "schema",
    "headless"
  ],
  "homepage": "https://dossierhq.dev",
  "author": {
    "name": "Jonas Bengtsson"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./lib/index.js",
    "./main.css": "./lib/main.css"
  },
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "npm run prettier:check && heft build --clean && npm run eslint:check && npm run test",
    "build:watch": "heft build-watch",
    "clean": "heft clean",
    "eslint:check": "eslint . --max-warnings 0",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "start": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build --quiet",
    "test": "vitest",
    "test:update-snapshot": "vitest --update",
    "test:watch": "vitest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dossierhq/dossierhq.git",
    "directory": "libraries/react-components"
  },
  "dependencies": {
    "@dossierhq/design": "workspace:*",
    "@dossierhq/leaflet": "workspace:*",
    "@lexical/code": "0.14.3",
    "@lexical/link": "0.14.3",
    "@lexical/list": "0.14.3",
    "@lexical/react": "0.14.3",
    "@lexical/rich-text": "0.14.3",
    "@lexical/selection": "0.14.3",
    "@lexical/utils": "0.14.3",
    "leaflet": "~1.9.4",
    "lexical": "0.14.3",
    "lodash": "~4.17.21",
    "swr": "~2.2.5",
    "tslib": "~2.6.2"
  },
  "peerDependencies": {
    "@dossierhq/core": "workspace:*",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@dossierhq/better-sqlite3": "workspace:*",
    "@dossierhq/core": "workspace:*",
    "@dossierhq/heft-node-rig": "workspace:*",
    "@dossierhq/server": "workspace:*",
    "@ianvs/prettier-plugin-sort-imports": "~4.2.1",
    "@rushstack/heft": "0.66.9",
    "@storybook/addon-actions": "~8.1.4",
    "@storybook/addon-essentials": "~8.1.4",
    "@storybook/addon-links": "~8.1.4",
    "@storybook/react": "~8.1.4",
    "@storybook/react-vite": "~8.1.4",
    "@storybook/test": "~8.1.4",
    "@types/leaflet": "~1.9.12",
    "@types/lodash": "~4.17.3",
    "@types/react-dom": "~18.3.0",
    "@types/react": "18.3.3",
    "@types/uuid": "~9.0.7",
    "@typescript-eslint/eslint-plugin": "~7.10.0",
    "@typescript-eslint/parser": "~7.10.0",
    "@vitejs/plugin-react": "~4.2.1",
    "eslint": "~8.57.0",
    "eslint-plugin-import": "~2.29.1",
    "eslint-plugin-react": "~7.34.1",
    "eslint-plugin-react-hooks": "~4.6.2",
    "eslint-plugin-storybook": "~0.8.0",
    "playground-example-generator": "workspace:*",
    "prettier": "3.2.5",
    "react": "~18.3.1",
    "react-dom": "~18.3.1",
    "storybook": "~8.1.4",
    "typescript": "~5.4.5",
    "uuid": "~9.0.1",
    "vite": "~5.2.11",
    "vitest": "~1.6.0"
  }
}
