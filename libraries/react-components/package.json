{
  "name": "@dossierhq/react-components",
  "version": "0.7.21",
  "description": "React components for creating a headless CMS using Dossier.",
  "keywords": [
    "dossier",
    "cms",
    "schema",
    "headless"
  ],
  "homepage": "https://dossierhq.dev",
  "author": {
    "name": "Jonas Bengtsson"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./lib/index.js",
    "./main.css": "./lib/main.css"
  },
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && cp src/main.css lib/main.css",
    "build:watch": "tsc --watch",
    "lint": "eslint --max-warnings=0",
    "lint:staged": "lint-staged",
    "clean": "rm -rf lib/",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "format": "prettier --write .",
    "start": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build --quiet",
    "test": "vitest",
    "test:update-snapshot": "vitest --update",
    "test:watch": "vitest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dossierhq/dossierhq.git",
    "directory": "libraries/react-components"
  },
  "dependencies": {
    "@dossierhq/design": "workspace:*",
    "@dossierhq/leaflet": "workspace:*",
    "@lexical/code": "0.40.0",
    "@lexical/link": "0.40.0",
    "@lexical/list": "0.40.0",
    "@lexical/react": "0.40.0",
    "@lexical/rich-text": "0.40.0",
    "@lexical/selection": "0.40.0",
    "@lexical/utils": "0.40.0",
    "leaflet": "~1.9.4",
    "lexical": "0.40.0",
    "lodash": "~4.17.23",
    "swr": "~2.3.8",
    "tslib": "~2.8.1"
  },
  "peerDependencies": {
    "@dossierhq/core": "workspace:*",
    "react": ">=19",
    "react-dom": ">=19"
  },
  "devDependencies": {
    "@dossierhq/better-sqlite3": "workspace:*",
    "@dossierhq/core": "workspace:*",
    "@dossierhq/eslint-config": "workspace:*",
    "@dossierhq/server": "workspace:*",
    "@dossierhq/test-data": "workspace:*",
    "@dossierhq/typescript-config": "workspace:*",
    "@storybook/addon-docs": "~10.2.9",
    "@storybook/addon-links": "~10.2.9",
    "@storybook/react-vite": "~10.2.9",
    "@types/leaflet": "~1.9.14",
    "@types/lodash": "~4.17.23",
    "@types/react": "19.2.14",
    "@types/react-dom": "~19.2.3",
    "@vitejs/plugin-react": "~5.1.4",
    "better-sqlite3": "~12.6.2",
    "body-parser": "~2.2.2",
    "eslint": "~9.39.2",
    "react": "~19.2.4",
    "react-dom": "~19.2.4",
    "storybook": "~10.2.9",
    "typescript": "~5.9.3",
    "uuid": "~13.0.0",
    "vite": "7.3.1",
    "vitest": "~4.0.18"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,cjs,mjs}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "*.css": "prettier --write"
  }
}
