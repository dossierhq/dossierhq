{
  "name": "@dossierhq/leaflet",
  "version": "0.3.1",
  "description": "A library for rendering maps in Dossier with Leaflet.",
  "keywords": [
    "dossier",
    "cms",
    "leaflet",
    "map",
    "content"
  ],
  "homepage": "https://dossierhq.dev",
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./icons.css": "./lib/icons.css",
    "./main.css": "./lib/main.css"
  },
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "npm run prettier:check && heft build --clean",
    "build:watch": "heft build-watch",
    "clean": "heft clean",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix .",
    "icons:update": "scripts/generate-icons.js",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dossierhq/dossierhq.git",
    "directory": "libraries/leaflet"
  },
  "dependencies": {
    "leaflet": "~1.9.4",
    "leaflet.locatecontrol": "~0.79.0",
    "react": "~18.2.0",
    "react-leaflet": "~4.2.1",
    "tslib": "~2.6.0"
  },
  "devDependencies": {
    "@dossierhq/heft-node-rig": "workspace:*",
    "@dossierhq/eslint-config-generic": "workspace:*",
    "@rushstack/heft": "0.56.0",
    "@types/leaflet": "~1.9.3",
    "@types/leaflet.locatecontrol": "~0.74.1",
    "@types/react": "~18.2.14",
    "eslint": "~8.44.0",
    "mini-svg-data-uri": "~1.4.4",
    "prettier": "2.8.8",
    "svgo": "~3.0.2",
    "typescript": "~5.1.6"
  }
}
