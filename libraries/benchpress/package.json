{
  "name": "benchpress",
  "version": "0.1.0",
  "description": "",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": {
        "types": "./lib/esm/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "types": "./lib/esm/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "npm run prettier:check && heft build --clean && post-process-build deno cjs-package-json && npm run test",
    "build:watch": "heft build-watch",
    "clean": "heft clean",
    "deno:reload-dependencies": "deno cache --reload deno-external-dependencies.ts",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix .",
    "examples:timer-benchmark:bun": "bun run ./src/examples/timer-benchmark.ts",
    "examples:timer-benchmark:deno": "deno run --allow-env --allow-hrtime --allow-run=gnuplot --allow-read --allow-write=output ./lib/esm/examples/timer-benchmark.js",
    "examples:timer-benchmark:node": "node ./lib/esm/examples/timer-benchmark.js",
    "postinstall": "deno cache deno-external-dependencies.ts",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test": "npm run examples:timer-benchmark:bun && npm run examples:timer-benchmark:deno && npm run examples:timer-benchmark:node"
  },
  "devDependencies": {
    "@dossierhq/heft-node-rig": "workspace:*",
    "@dossierhq/eslint-config-generic": "workspace:*",
    "@rushstack/heft": "0.56.0",
    "@types/node": "~20.3.3",
    "eslint": "~8.44.0",
    "prettier": "3.0.0",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "tslib": "~2.6.0"
  }
}
