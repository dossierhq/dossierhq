{
  "compilerOptions": {
    "target": "es2022",
    "lib": ["ES2023", "DOM"],
    "module": "ES2022",
    "moduleResolution": "bundler",
    "declaration": true,
    "sourceMap": true,
    "importHelpers": true,
    "verbatimModuleSyntax": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "skipLibCheck": true,
    "isolatedDeclarations": true
  }
}
