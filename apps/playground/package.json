{
  "name": "playground",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "vite --force --port 3002",
    "build": "./scripts/sync-dossier-docs-database.sh && vite build",
    "check-types": "tsc",
    "lint": "eslint --max-warnings=0",
    "lint:staged": "lint-staged",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "format": "prettier --write .",
    "preview": "vite --port 3002 preview"
  },
  "dependencies": {
    "@dossierhq/cloudinary": "workspace:*",
    "@dossierhq/core": "workspace:*",
    "@dossierhq/design": "workspace:*",
    "@dossierhq/graphql": "workspace:*",
    "@dossierhq/leaflet": "workspace:*",
    "@dossierhq/react-components": "workspace:*",
    "@dossierhq/server": "workspace:*",
    "@dossierhq/sql.js": "workspace:*",
    "@dossierhq/test-data": "workspace:*",
    "graphiql": "~5.2.2",
    "graphql": "~16.12.0",
    "leaflet": "~1.9.4",
    "react": "~19.2.4",
    "react-dom": "~19.2.4",
    "react-router-dom": "~7.12.0",
    "sql.js": "~1.13.0",
    "swr": "~2.3.8"
  },
  "devDependencies": {
    "@dossierhq/eslint-config": "workspace:*",
    "@graphiql/toolkit": "~0.11.1",
    "@types/react": "19.2.14",
    "@types/react-dom": "~19.2.3",
    "@types/sql.js": "~1.4.9",
    "@vitejs/plugin-react-swc": "~4.2.3",
    "eslint": "~10.0.0",
    "graphql-ws": "~6.0.7",
    "rollup-plugin-visualizer": "~6.0.1",
    "tsx": "~4.21.0",
    "typescript": "~5.9.3",
    "vite": "7.3.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,cjs,mjs}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ]
  }
}
