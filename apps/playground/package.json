{
  "name": "playground",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "vite --force --port 3002",
    "build": "./scripts/sync-dossier-docs-database.sh && vite build",
    "check-types": "tsc",
    "lint": "npm run prettier:check && npm run eslint:check",
    "eslint:check": "eslint --max-warnings=0",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "preview": "vite --port 3002 preview"
  },
  "dependencies": {
    "@dossierhq/cloudinary": "workspace:*",
    "@dossierhq/core": "workspace:*",
    "@dossierhq/design": "workspace:*",
    "@dossierhq/graphql": "workspace:*",
    "@dossierhq/leaflet": "workspace:*",
    "@dossierhq/react-components": "workspace:*",
    "@dossierhq/server": "workspace:*",
    "@dossierhq/sql.js": "workspace:*",
    "@dossierhq/test-data": "workspace:*",
    "graphiql": "~4.1.0",
    "graphql": "~16.11.0",
    "leaflet": "~1.9.4",
    "react": "~19.2.0",
    "react-dom": "~19.2.0",
    "react-router-dom": "~7.9.6",
    "sql.js": "~1.13.0",
    "swr": "~2.3.3"
  },
  "devDependencies": {
    "@dossierhq/eslint-config": "workspace:*",
    "@graphiql/toolkit": "~0.11.1",
    "@types/react": "19.2.6",
    "@types/react-dom": "~19.2.3",
    "@types/sql.js": "~1.4.9",
    "@vitejs/plugin-react-swc": "~4.2.2",
    "eslint": "~9.39.1",
    "graphql-ws": "~6.0.6",
    "rollup-plugin-visualizer": "~6.0.1",
    "tsx": "~4.20.3",
    "typescript": "~5.9.3",
    "vite": "7.2.2"
  }
}
