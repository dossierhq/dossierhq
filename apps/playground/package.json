{
  "name": "playground",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "vite --force --port 3002",
    "build": "./scripts/sync-dossier-docs-database.sh && vite build",
    "check-types": "tsc",
    "lint": "npm run prettier:check && npm run eslint:check",
    "eslint:check": "eslint --max-warnings=0",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "preview": "vite --port 3002 preview"
  },
  "dependencies": {
    "@dossierhq/cloudinary": "workspace:*",
    "@dossierhq/core": "workspace:*",
    "@dossierhq/design": "workspace:*",
    "@dossierhq/graphql": "workspace:*",
    "@dossierhq/leaflet": "workspace:*",
    "@dossierhq/react-components": "workspace:*",
    "@dossierhq/server": "workspace:*",
    "@dossierhq/sql.js": "workspace:*",
    "@dossierhq/test-data": "workspace:*",
    "graphiql": "~3.8.3",
    "graphql": "~16.10.0",
    "history": "~5.3.0",
    "leaflet": "~1.9.4",
    "react": "~19.0.0",
    "react-dom": "~19.0.0",
    "react-router-dom": "~7.3.0",
    "sql.js": "~1.12.0",
    "swr": "~2.3.3"
  },
  "devDependencies": {
    "@dossierhq/eslint-config": "workspace:*",
    "@graphiql/toolkit": "~0.11.1",
    "@types/react": "19.0.10",
    "@types/react-dom": "~19.0.4",
    "@types/sql.js": "~1.4.9",
    "@vitejs/plugin-react-swc": "~3.8.0",
    "eslint": "~9.21.0",
    "graphql-config": "~5.1.3",
    "graphql-ws": "~6.0.4",
    "rollup-plugin-visualizer": "~5.14.0",
    "tsx": "~4.19.3",
    "typescript": "~5.8.2",
    "vite": "~6.2.1"
  }
}
