{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "./scripts/generate-typescript.js 2>&1 && tsx ./scripts/sync-database-with-disk.ts && npm run copy-public-resources && npm run prettier:check && npm run eslint:check && npm run test && npm run build-app 2>&1 && npx tsx --tsconfig ./tsconfig.tsx.json ./scripts/generate-atom-xml.tsx",
    "clean": "rm -rf .next/ && rm -f blog.build.log tsconfig.tsbuildinfo",
    "copy-public-resources": "cp node_modules/@antonz/codapi/dist/snippet.js public/codapi-snippet.js && cp node_modules/codejar/dist/codejar.js public/ && cp node_modules/prismjs/prism.js public/",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "build-app": "next build",
    "start": "next dev --port 3003",
    "start:built": "next start",
    "test": "vitest",
    "test:update-snapshot": "vitest --update",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@antonz/codapi": "~0.19.5",
    "@cloudinary/url-gen": "~1.19.0",
    "@dossierhq/better-sqlite3": "workspace:*",
    "@dossierhq/cloudinary": "workspace:*",
    "@dossierhq/core": "workspace:*",
    "@dossierhq/design": "workspace:*",
    "@dossierhq/leaflet": "workspace:*",
    "@dossierhq/react-components": "workspace:*",
    "@dossierhq/server": "workspace:*",
    "better-sqlite3": "~9.6.0",
    "codejar": "~4.2.0",
    "leaflet": "~1.9.4",
    "lexical": "0.14.3",
    "next": "~14.2.3",
    "prismjs": "~1.29.0",
    "react": "~18.3.0",
    "react-dom": "~18.3.0",
    "tslib": "~2.6.2"
  },
  "devDependencies": {
    "@dossierhq/typescript-generator": "workspace:*",
    "@types/better-sqlite3": "~7.6.10",
    "@types/react": "18.3.0",
    "@types/react-dom": "~18.3.0",
    "@typescript-eslint/eslint-plugin": "~7.8.0",
    "dotenv": "~16.4.5",
    "eslint": "~8.57.0",
    "eslint-config-next": "~14.2.3",
    "prettier": "3.2.5",
    "schema-dts": "~1.1.2",
    "tsx": "~4.9.0",
    "typescript": "~5.4.5",
    "vitest": "~1.5.2"
  }
}
