{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "./scripts/generate-typescript.js 2>&1 && ts-node -T --esm ./scripts/sync-database-with-disk.ts && npm run prettier:check && npm run eslint:check && npm run test && npm run build-app 2>&1 && ts-node -T --esm ./scripts/generate-atom-xml.tsx",
    "clean": "rm -rf .next/ && rm -f blog.build.log tsconfig.tsbuildinfo",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "build-app": "next build",
    "start": "next dev --port 3003",
    "start:built": "next start",
    "test": "vitest",
    "test:update-snapshot": "vitest --update",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@cloudinary/url-gen": "~1.11.2",
    "@dossierhq/better-sqlite3": "workspace:*",
    "@dossierhq/cloudinary": "workspace:*",
    "@dossierhq/core": "workspace:*",
    "@dossierhq/design": "workspace:*",
    "@dossierhq/design-ssr": "workspace:*",
    "@dossierhq/leaflet": "workspace:*",
    "@dossierhq/react-components": "workspace:*",
    "@dossierhq/server": "workspace:*",
    "better-sqlite3": "~8.6.0",
    "leaflet": "~1.9.4",
    "lexical": "0.12.2",
    "next": "~13.4.19",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "tslib": "~2.6.2"
  },
  "devDependencies": {
    "@dossierhq/typescript-generator": "workspace:*",
    "@types/better-sqlite3": "~7.6.4",
    "@types/react": "~18.2.21",
    "@types/react-dom": "~18.2.7",
    "@typescript-eslint/eslint-plugin": "~6.7.0",
    "dotenv": "~16.3.1",
    "eslint": "~8.49.0",
    "eslint-config-next": "~13.4.19",
    "prettier": "3.0.3",
    "schema-dts": "~1.1.2",
    "ts-node": "~10.9.1",
    "typescript": "~5.2.2",
    "vitest": "~0.34.4"
  }
}
