{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "./scripts/generate-typescript.js 2>&1 && tsx ./scripts/sync-database-with-disk.ts && npm run prettier:check && npm run eslint:check && npm run test && npm run build-app 2>&1 && npx tsx --tsconfig ./tsconfig.tsx.json ./scripts/generate-atom-xml.tsx",
    "clean": "rm -rf .next/ && rm -f blog.build.log tsconfig.tsbuildinfo",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "build-app": "next build",
    "start": "next dev --port 3003",
    "start:built": "next start",
    "test": "vitest",
    "test:update-snapshot": "vitest --update",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@cloudinary/url-gen": "~1.18.0",
    "@dossierhq/better-sqlite3": "workspace:*",
    "@dossierhq/cloudinary": "workspace:*",
    "@dossierhq/core": "workspace:*",
    "@dossierhq/design": "workspace:*",
    "@dossierhq/leaflet": "workspace:*",
    "@dossierhq/react-components": "workspace:*",
    "@dossierhq/server": "workspace:*",
    "better-sqlite3": "~9.4.3",
    "leaflet": "~1.9.4",
    "lexical": "0.13.1",
    "next": "~14.1.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "tslib": "~2.6.2"
  },
  "devDependencies": {
    "@dossierhq/typescript-generator": "workspace:*",
    "@types/better-sqlite3": "~7.6.8",
    "@types/react": "~18.2.58",
    "@types/react-dom": "~18.2.19",
    "@typescript-eslint/eslint-plugin": "~7.1.0",
    "dotenv": "~16.4.5",
    "eslint": "~8.57.0",
    "eslint-config-next": "~14.1.0",
    "prettier": "3.2.5",
    "schema-dts": "~1.1.2",
    "tsx": "~4.7.1",
    "typescript": "~5.3.3",
    "vitest": "~1.3.1"
  }
}
