{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "./scripts/generate-typescript.js 2>&1 && ./scripts/create-database-if-missing.sh && npm run prettier:check && npm run eslint:check && npm run test && npm run build-app 2>&1",
    "clean": "rm -rf .next/ && rm -f blog.build.log tsconfig.tsbuildinfo",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "build-app": "cp node_modules/graphql-voyager/dist/voyager.worker.js public/ && cp node_modules/sql.js/dist/sql-wasm.wasm public/ && next build",
    "start": "cp node_modules/graphql-voyager/dist/voyager.worker.js public/ && cp node_modules/sql.js/dist/sql-wasm.wasm public/ && next dev --port 3003",
    "start:built": "next start",
    "test": "vitest",
    "test:update-snapshot": "vitest --update",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@jonasb/datadata-admin-react-components": "workspace:*",
    "@jonasb/datadata-core": "workspace:*",
    "@jonasb/datadata-database-adapter-sqlite-sql.js": "workspace:*",
    "@jonasb/datadata-database-adapter-sqlite-sqlite3": "workspace:*",
    "@jonasb/datadata-design": "workspace:*",
    "@jonasb/datadata-design-server": "workspace:*",
    "@jonasb/datadata-graphql": "workspace:*",
    "@jonasb/datadata-leaflet": "workspace:*",
    "@jonasb/datadata-server": "workspace:*",
    "graphql": "~16.6.0",
    "graphql-voyager": "~1.0.0-rc.31",
    "leaflet": "~1.9.1",
    "lexical": "~0.7.0",
    "next": "~13.0.1",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "sql.js": "~1.8.0",
    "sqlite3": "~5.1.2",
    "swr": "~2.0.0"
  },
  "devDependencies": {
    "@jonasb/datadata-typescript-generator": "workspace:*",
    "@types/react": "~18.0.26",
    "@types/sql.js": "~1.4.4",
    "@types/sqlite3": "~3.1.8",
    "@typescript-eslint/eslint-plugin": "~5.47.0",
    "eslint": "~8.30.0",
    "eslint-config-next": "~13.0.1",
    "prettier": "2.8.1",
    "ts-node": "~10.9.1",
    "typescript": "~4.9.4",
    "vitest": "0.26.1"
  }
}
