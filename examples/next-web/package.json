{
  "name": "datadata-examples-next-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run prettier:check && npm run ts:check && npm run eslint:check && npm run test && npm run build-app",
    "clean": "rm -rf .next/ && rm -f datadata-examples-next-web.build.log tsconfig.tsbuildinfo",
    "db:cli": "./scripts/db-cli.sh",
    "db:migrate": "./scripts/db-migrate.sh",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "build-app": "cp node_modules/graphql-voyager/dist/voyager.worker.js public/ && next build",
    "start": "cp node_modules/graphql-voyager/dist/voyager.worker.js public/ && next dev --port 3001",
    "start:sqlite": "cp node_modules/graphql-voyager/dist/voyager.worker.js public/ && DATABASE_SQLITE_FILE=data/database.sqlite next dev --port 3001",
    "start:built": "next start",
    "start:cli": "env TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' npx ts-node cli.ts",
    "test": "vitest",
    "test:update-snapshot": "vitest --update",
    "test:watch": "vitest --watch",
    "ts:check": "tsc --noEmit",
    "ts:check:watch": "tsc --noEmit --watch",
    "upgrade-dependencies": "npx npm-check-updates -u --packageFile ./package.json",
    "upgrade-dependencies:check": "npx npm-check-updates"
  },
  "dependencies": {
    "@jonasb/datadata-admin-react-components": "workspace:*",
    "@jonasb/datadata-cli": "workspace:*",
    "@jonasb/datadata-core": "workspace:*",
    "@jonasb/datadata-design": "workspace:*",
    "@jonasb/datadata-database-adapter-postgres-pg": "workspace:*",
    "@jonasb/datadata-database-adapter-sqlite-sqlite3": "workspace:*",
    "@jonasb/datadata-graphql": "workspace:*",
    "@jonasb/datadata-server": "workspace:*",
    "@editorjs/checklist": "~1.3.0",
    "@editorjs/header": "~2.6.2",
    "@editorjs/list": "~1.7.0",
    "@editorjs/quote": "~2.4.0",
    "@editorjs/inline-code": "~1.3.1",
    "@editorjs/underline": "~1.0.0",
    "@hapi/boom": "~10.0.0",
    "debug": "~4.3.4",
    "joi": "~17.6.0",
    "graphql": "~16.5.0",
    "graphql-voyager": "~1.0.0-rc.31",
    "leaflet": "~1.8.0",
    "next": "~12.1.4",
    "react": "~18.1.0",
    "react-dom": "~18.1.0",
    "sqlite3": "jonasb/node-sqlite3#3.36.0",
    "swr": "~1.3.0"
  },
  "devDependencies": {
    "@jonasb/datadata-postgres": "workspace:*",
    "@types/debug": "~4.1.7",
    "@types/react": "~18.0.0",
    "@types/sqlite3": "~3.1.8",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-next": "~12.1.4",
    "prettier": "2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.7.2",
    "vitest": "~0.13.0"
  }
}
