{
  "name": "@dossierhq/example-next-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "npm run prettier:check && npm run eslint:check && npm run build-app",
    "build-app": "cp node_modules/graphql-voyager/dist/voyager.worker.js public/ && next build",
    "clean": "rm -rf .next/ && rm -f example-next-web.build.log tsconfig.tsbuildinfo",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "start": "cp node_modules/graphql-voyager/dist/voyager.worker.js public/ && next dev --port 3001",
    "start:built": "next start"
  },
  "dependencies": {
    "@dossierhq/better-sqlite3": "workspace:*",
    "@dossierhq/core": "workspace:*",
    "@dossierhq/design": "workspace:*",
    "@dossierhq/graphql": "workspace:*",
    "@dossierhq/leaflet": "workspace:*",
    "@dossierhq/react-components": "workspace:*",
    "@dossierhq/server": "workspace:*",
    "better-sqlite3": "~8.2.0",
    "graphql-voyager": "~1.0.0-rc.31",
    "graphql": "~16.6.0",
    "leaflet": "~1.9.3",
    "next": "~13.2.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "~7.6.3",
    "@types/react": "~18.0.28",
    "@typescript-eslint/eslint-plugin": "~5.52.0",
    "eslint": "~8.34.0",
    "eslint-config-next": "~13.2.0",
    "prettier": "2.8.4",
    "typescript": "~4.9.5"
  }
}
