{
  "name": "datadata-examples-next-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "npm run prettier:check && npm run eslint:check && npm run test && npm run build-app",
    "clean": "rm -rf .next/ && rm -f datadata-examples-next-web.build.log tsconfig.tsbuildinfo",
    "db:migrate": "./scripts/db-migrate.sh",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "build-app": "cp node_modules/graphql-voyager/dist/voyager.worker.js public/ && next build",
    "start": "cp node_modules/graphql-voyager/dist/voyager.worker.js public/ && next dev --port 3001",
    "start:sqlite": "cp node_modules/graphql-voyager/dist/voyager.worker.js public/ && DATABASE_SQLITE_FILE=data/database.sqlite next dev --port 3001",
    "start:built": "next start",
    "test": "vitest",
    "test:update-snapshot": "vitest --update",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@dossierhq/react-components": "workspace:*",
    "@dossierhq/core": "workspace:*",
    "@dossierhq/pg": "workspace:*",
    "@dossierhq/sqlite3": "workspace:*",
    "@dossierhq/design": "workspace:*",
    "@jonasb/datadata-graphql": "workspace:*",
    "@jonasb/datadata-leaflet": "workspace:*",
    "@dossierhq/server": "workspace:*",
    "graphql": "~16.6.0",
    "graphql-voyager": "~1.0.0-rc.31",
    "leaflet": "~1.9.3",
    "next": "~13.1.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "sqlite3": "~5.1.2"
  },
  "devDependencies": {
    "@jonasb/datadata-postgres": "workspace:*",
    "@types/react": "~18.0.26",
    "@types/sqlite3": "~3.1.8",
    "@typescript-eslint/eslint-plugin": "~5.48.0",
    "eslint": "~8.32.0",
    "eslint-config-next": "~13.1.0",
    "prettier": "2.8.3",
    "typescript": "~4.9.4",
    "vitest": "0.27.1"
  }
}
