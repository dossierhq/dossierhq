{
  "name": "datadata-examples-next-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "ci:check": "npm run ts:check && npm run eslint:check && npm run prettier:check && npm run build",
    "db:cli": "./scripts/db-cli.sh",
    "db:migrate": "./scripts/db-migrate.sh",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "dev": "cp node_modules/graphql-voyager/dist/voyager.worker.js public/ && next dev",
    "build": "cp node_modules/graphql-voyager/dist/voyager.worker.js public/ && next build",
    "start": "next start",
    "start:cli": "env TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' npx ts-node cli.ts",
    "test": "jest",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "ts:check": "tsc --noEmit",
    "upgrade-dependencies": "npx npm-check-updates -u --packageFile ./package.json",
    "upgrade-dependencies:check": "npx npm-check-updates"
  },
  "dependencies": {
    "@datadata/admin-react-components": "1.0.0",
    "@datadata/cli": "1.0.0",
    "@datadata/core": "1.0.0",
    "@datadata/graphql": "1.0.0",
    "@datadata/server": "1.0.0",
    "@editorjs/checklist": "~1.3.0",
    "@editorjs/header": "~2.6.1",
    "@editorjs/list": "~1.6.2",
    "@editorjs/quote": "~2.4.0",
    "@editorjs/inline-code": "~1.3.1",
    "@editorjs/underline": "~1.0.0",
    "@hapi/boom": "~9.1.2",
    "debug": "~4.3.1",
    "joi": "~17.4.0",
    "graphql": "~15.5.0",
    "graphql-voyager": "~1.0.0-rc.31",
    "next": "~10.0.6",
    "next-transpile-modules": "~6.1.0",
    "react": "~17.0.2",
    "react-dom": "~17.0.2"
  },
  "devDependencies": {
    "@datadata/postgres": "1.0.0",
    "@types/debug": "~4.1.5",
    "@types/jest": "~26.0.22",
    "@types/node": "^14.14.37",
    "@types/react": "~17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "eslint-plugin-react": "~7.23.2",
    "eslint-plugin-react-hooks": "~4.2.0",
    "jest": "~26.6.3",
    "prettier": "2.2.1",
    "ts-jest": "~26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
