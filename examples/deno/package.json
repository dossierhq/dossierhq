{
  "name": "datadata-examples-deno",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "deno lint -q && (deno fmt --check 2>&1) && (deno cache --import-map config/import-map.json server.ts 2>&1) && (npm run test 2>&1)",
    "db:cli": "./scripts/db-cli.sh",
    "db:migrate": "./scripts/db-migrate.sh",
    "postinstall": "deno cache --import-map config/import-map.json external-dependencies.ts",
    "test": "deno test -j --import-map=./config/import-map.json --allow-net=localhost:5432 --allow-read=.env,.env.defaults --allow-env=GITHUB_ACTIONS",
    "test:watch": "deno test --watch --import-map=./config/import-map.json"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jonasb/datadata-core": "workspace:*",
    "@jonasb/datadata-database-adapter": "workspace:*",
    "@jonasb/datadata-database-adapter-postgres-core": "workspace:*",
    "@jonasb/datadata-database-adapter-test-integration": "workspace:*",
    "@jonasb/datadata-postgres": "workspace:*",
    "@jonasb/datadata-server": "workspace:*"
  }
}
