{
  "name": "@dossierhq/example-benchmark",
  "version": "0.1.0",
  "description": "",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "npm run prettier:check && npm run eslint:check",
    "clean": "rm -rf lib/",
    "db:cli": "./scripts/db-cli.sh",
    "db:migrate": "./scripts/db-migrate.sh",
    "deno:reload-dependencies": "deno cache --reload deno-external-dependencies.ts",
    "eslint:check": "eslint --max-warnings=0",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "start": "node lib/run-node-pg.js && node lib/run-node-better-sqlite3.js && bun run lib/run-bun-sqlite.js && deno run --allow-all deno/run-deno-postgres.ts",
    "start:bun-sqlite": "bun run lib/run-bun-sqlite.js",
    "start:deno-postgres": "deno run --allow-all deno/run-deno-postgres.ts",
    "start:node-better-sqlite3": "node lib/run-node-better-sqlite3.js",
    "start:node-pg": "node lib/run-node-pg.js",
    "start:ci": "node lib/run-node-pg.js ci && node lib/run-node-better-sqlite3.js ci && bun run lib/run-bun-sqlite.js ci && deno run --allow-all deno/run-deno-postgres.ts ci"
  },
  "dependencies": {
    "@dossierhq/better-sqlite3": "workspace:*",
    "@dossierhq/bun-sqlite": "workspace:*",
    "@dossierhq/core": "workspace:*",
    "@dossierhq/database-adapter": "workspace:*",
    "@dossierhq/deno-postgres": "workspace:*",
    "@dossierhq/pg": "workspace:*",
    "@dossierhq/postgres-core": "workspace:*",
    "@dossierhq/server": "workspace:*",
    "@faker-js/faker": "~9.6.0",
    "benchpress": "workspace:*",
    "better-sqlite3": "~11.8.1",
    "dotenv": "~16.4.7",
    "pg": "~8.13.3",
    "tslib": "~2.8.1"
  },
  "devDependencies": {
    "@dossierhq/eslint-config": "workspace:*",
    "@dossierhq/postgres-tools": "workspace:*",
    "@dossierhq/typescript-config": "workspace:*",
    "@types/better-sqlite3": "~7.6.12",
    "@types/bun": "~1.2.2",
    "@types/pg": "~8.11.11",
    "eslint": "~9.22.0",
    "typescript": "~5.8.2"
  }
}
