{
  "name": "@dossierhq/example-benchmark",
  "version": "0.1.0",
  "description": "",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "npm run prettier:check && heft build --clean",
    "build:watch": "heft build --watch",
    "clean": "heft clean",
    "db:cli": "./scripts/db-cli.sh",
    "db:migrate": "./scripts/db-migrate.sh",
    "deno:reload-dependencies": "deno cache --reload deno-external-dependencies.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "start": "node lib/run-node-pg.js && node lib/run-node-better-sqlite3.js && node lib/run-node-sqlite3.js && bun run lib/run-bun-sqlite.js && deno run --allow-all deno/run-deno-postgres.ts",
    "start:bun-sqlite": "bun run lib/run-bun-sqlite.js",
    "start:deno-postgres": "deno run --allow-all deno/run-deno-postgres.ts",
    "start:node-better-sqlite3": "node lib/run-node-better-sqlite3.js",
    "start:node-pg": "node lib/run-node-pg.js",
    "start:node-sqlite3": "node lib/run-node-sqlite3.js",
    "start:ci": "node lib/run-node-pg.js ci && node lib/run-node-sqlite3.js ci && bun run lib/run-bun-sqlite.js ci && deno run --allow-all deno/run-deno-postgres.ts ci"
  },
  "dependencies": {
    "@faker-js/faker": "~8.0.1",
    "@dossierhq/core": "workspace:*",
    "@dossierhq/database-adapter": "workspace:*",
    "@dossierhq/better-sqlite3": "workspace:*",
    "@dossierhq/postgres-core": "workspace:*",
    "@dossierhq/deno-postgres": "workspace:*",
    "@dossierhq/pg": "workspace:*",
    "@dossierhq/bun-sqlite": "workspace:*",
    "@dossierhq/sqlite3": "workspace:*",
    "@dossierhq/server": "workspace:*",
    "benchpress": "workspace:*",
    "better-sqlite3": "~8.4.0",
    "dotenv": "~16.0.3",
    "pg": "~8.11.0",
    "sqlite3": "5.1.4",
    "tslib": "~2.5.2"
  },
  "devDependencies": {
    "@dossierhq/heft-node-rig": "workspace:*",
    "@dossierhq/postgres-tools": "workspace:*",
    "@dossierhq/eslint-config-generic": "workspace:*",
    "@rushstack/heft": "0.50.5",
    "@types/better-sqlite3": "~7.6.4",
    "@types/pg": "~8.6.6",
    "@types/sqlite3": "~3.1.8",
    "eslint": "~8.41.0",
    "prettier": "2.8.8",
    "typescript": "~5.0.4"
  }
}
