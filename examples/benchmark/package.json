{
  "name": "datadata-examples-benchmark",
  "version": "0.1.0",
  "description": "",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "npm run prettier:check && heft build --clean",
    "build:watch": "heft build --watch",
    "clean": "heft clean",
    "db:cli": "./scripts/db-cli.sh",
    "db:migrate": "./scripts/db-migrate.sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "start": "node lib/run-node-pg.js && node lib/run-node-better-sqlite3.js && node lib/run-node-sqlite3.js && bun run lib/run-bun-sqlite.js && deno run --allow-all deno/run-deno-postgres.ts",
    "start:bun-sqlite": "bun run lib/run-bun-sqlite.js",
    "start:deno-postgres": "deno run --allow-all deno/run-deno-postgres.ts",
    "start:node-better-sqlite3": "node lib/run-node-better-sqlite3.js",
    "start:node-pg": "node lib/run-node-pg.js",
    "start:node-sqlite3": "node lib/run-node-sqlite3.js",
    "start:ci": "node lib/run-node-pg.js ci && node lib/run-node-sqlite3.js ci && bun run lib/run-bun-sqlite.js ci && deno run --allow-all deno/run-deno-postgres.ts ci"
  },
  "dependencies": {
    "@faker-js/faker": "~7.6.0",
    "@jonasb/datadata-core": "workspace:*",
    "@jonasb/datadata-database-adapter": "workspace:*",
    "@jonasb/datadata-database-adapter-better-sqlite3": "workspace:*",
    "@jonasb/datadata-database-adapter-postgres-core": "workspace:*",
    "@jonasb/datadata-database-adapter-postgres-deno": "workspace:*",
    "@jonasb/datadata-database-adapter-postgres-pg": "workspace:*",
    "@jonasb/datadata-database-adapter-sqlite-bun": "workspace:*",
    "@jonasb/datadata-database-adapter-sqlite-sqlite3": "workspace:*",
    "@jonasb/datadata-server": "workspace:*",
    "benchpress": "workspace:*",
    "better-sqlite3": "~8.0.1",
    "dotenv": "~16.0.3",
    "pg": "~8.8.0",
    "sqlite3": "~5.1.2",
    "tslib": "~2.4.1"
  },
  "devDependencies": {
    "@jonasb/datadata-heft-node-rig": "workspace:*",
    "@jonasb/datadata-postgres": "workspace:*",
    "@jonasb/eslint-config-datadata-generic": "workspace:*",
    "@rushstack/heft": "0.49.0",
    "@types/better-sqlite3": "~7.6.3",
    "@types/pg": "~8.6.5",
    "@types/sqlite3": "~3.1.8",
    "eslint": "~8.31.0",
    "prettier": "2.8.1",
    "typescript": "~4.9.4"
  }
}
