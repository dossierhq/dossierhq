{
  "name": "@jonasb/datadata-template-tool",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "npm run prettier:check && heft build --clean",
    "build:watch": "heft build --watch",
    "clean": "heft clean",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "start": "node lib/main.js"
  },
  "dependencies": {
    "tslib": "~2.4.0"
  },
  "devDependencies": {
    "@jonasb/datadata-heft-node-rig": "workspace:*",
    "@jonasb/eslint-config-datadata-generic": "workspace:*",
    "@rushstack/heft": "~0.45.5",
    "eslint": "^8.13.0",
    "prettier": "2.6.2",
    "typescript": "^4.7.2"
  }
}
